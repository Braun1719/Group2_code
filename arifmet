package com.mycompany.floatnumbers;
public class FloatNumbers {
public static float arifmet(float x, float y, float c, float a, float b){
    float result = x + y + c + a + b;
    return result/5;
} 
public static void main(String[] args) {
System.out.println(arifmet(8.5F,6.5F,7.7F,3.5F,2.6F));
}
}
